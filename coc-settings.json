{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "html",
    "scss",
    "css"
  ],
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.floatActions": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.hoverTarget": "preview",
  "prettier.requireConfig": true,
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "scs",
    "css",
    "java"
  ]
}
